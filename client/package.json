{
  "name": "loc8r",
  "version": "1.0.0",
  "description": "Loc8r - MEAN stack location finder web application",
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "NODE_ENV=test nodemon ./bin/www",
    "seed": "node seed.js",
    "build-client": "uglifyjs app_client/**/*.js -o public/javascripts/loc8r.min.js",
    "build-css": "sass public/stylesheets/style.scss public/stylesheets/style.css",
    "watch-css": "sass --watch public/stylesheets/style.scss:public/stylesheets/style.css",
    "build": "npm run build-client && npm run build-css",
    "dev-full": "concurrently \"npm run dev\" \"npm run watch-css\""
  },
  "keywords": [
    "mean",
    "mongodb",
    "express",
    "angular",
    "node",
    "location",
    "finder",
    "maps",
    "geolocation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^20.3.7",
    "@angular/compiler": "^20.3.7",
    "@angular/compiler-cli": "^20.3.7",
    "@angular/core": "^20.3.7",
    "@angular/forms": "^20.3.7",
    "@angular/platform-browser": "^20.3.7",
    "@angular/platform-browser-dynamic": "^20.3.7",
    "@angular/router": "^20.3.7",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.0",
    "rxjs": "^7.8.2",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "sass": "^1.92.1",
    "typescript": "^5.8.2",
    "uglify-js": "^3.17.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "your-repository-url"
  }
}
